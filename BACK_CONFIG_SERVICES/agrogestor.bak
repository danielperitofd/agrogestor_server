server {
    listen 80;
    server_name agrogestor.miroutepe.com.br www.agrogestor.miroutepe.com.br;


    location /static/ {
        alias /var/www/agrogestor_server/staticfiles/;
        autoindex on;
        try_files $uri $uri/ =404;
    }

    location / {
        proxy_pass http://127.0.0.1:8002;
        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
    }
}
